<Window x:Class="MysteryWorld.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:views="clr-namespace:MysteryWorld.Views"
        mc:Ignorable="d"
        Title="Sokoban" Width="1280" Height="1024" Background="#FF5E5E5E"
		FocusManager.FocusedElement="{Binding ElementName=level}">

    <views:LevelView x:Name="level" Focusable="True"></views:LevelView>

</Window>
